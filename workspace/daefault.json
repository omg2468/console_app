    "prog": [
        {
            "code": "//set(103,get(40)/100);\n//set(54,get(40)/10);\n//set(100,get(36)/100);\n//set(50,get(36)/10);\n//set(101,get(37)/100);\n//set(51,get(37)/10);\n//set(102,get(38)/100);\n//set(52,get(38)/10);\n//set(104,get(39)/100);\n//set(53,get(39)/10);\n\nset(55,get(5)*100);\nset(56,get(6)*100);\nset(57,get(7)*100);\nset(58,get(8)*100);\nset(50,get(106)*10);\nset(51,get(107)*10);\nset(52,get(109)*10);\nset(53,get(105)*10);\nset(54,get(108)*10);",
            "desc": "Set gia tri thanh ghi cho PLC",
            "en": false
        },
        {
            "code": "//CO\ngetint(41) == 2 ? \nsetint(216,1) : getint(41)==1?\nsetint(216,2) : getint(41)==3?\nsetint(216,2) : setint(216,0);\n//SO2\ngetint(41) == 2 ? \nsetint(207,1) : getint(41)==1?\nsetint(207,2) : getint(41)==3?\nsetint(207,2) : setint(207,0);\n//NOx\ngetint(41) == 2 ? \nsetint(206,1) : getint(41)==1?\nsetint(206,2) : getint(41)==3?\nsetint(206,2) : setint(206,0);\n//CO2\ngetint(41) == 2 ? \nsetint(205,1) : getint(41)==1?\nsetint(205,2) : getint(41)==3?\nsetint(205,2) : setint(205,0);\n//O2\ngetint(41) == 2 ? \nsetint(208,1) : getint(41)==1?\nsetint(208,2) : getint(41)==3?\nsetint(208,2) : setint(208,0);\n\n",
            "desc": "Status",
            "en": false
        },
        {
            "code": "// CO\nget(0) < 4 ? set(105,0) : get(0) >20 ? set(105,0) : set(105,(get(0)-4)*4000/16);\nget(0)<3.5?\nsetint(216,2) : getint(41) == 2 ? \nsetint(216,1) : getint(41)==1?\nsetint(216,2) : getint(41)==3?\nsetint(216,2) : setint(216,0);\n// CO2\nget(1) < 4 ? set(106,0) : get(1) >20 ? set(106,0) : set(106,(get(1)-4)*40/16);\nget(1)<3.5?\nsetint(205,2) : getint(41) == 2 ? \nsetint(205,1) : getint(41)==1?\nsetint(205,2) : getint(41)==3?\nsetint(205,2) : setint(205,0);\n// NOx\nget(2) < 4 ? set(107,0) : get(2) >20 ? set(107,0) : set(107,(get(2)-4)*3500/16);\nget(2)<3.5?\nsetint(206,2) : getint(41) == 2 ? \nsetint(206,1) : getint(41)==1?\nsetint(206,2) : getint(41)==3?\nsetint(206,2) : setint(206,0);\n// SO2\nget(3) < 4 ? set(108,0) : get(3) >20 ? set(108,0) : set(108,(get(3)-4)*3500/16);\nget(3)<3.5?\nsetint(207,2) : getint(41) == 2 ? \nsetint(207,1) : getint(41)==1?\nsetint(207,2) : getint(41)==3?\nsetint(207,2) : setint(207,0);\n// O2\nget(4) < 4 ? set(109,0) : get(4) >20 ? set(109,0) : set(109,(get(4)-4)*100/16);\nget(4)<3.5?\nsetint(208,2) : getint(41) == 2 ? \nsetint(208,1) : getint(41)==1?\nsetint(208,2) : getint(41)==3?\nsetint(208,2) : setint(208,0);\n// Dust\nget(5) < 4 ? set(110,0) : get(5) >20 ? set(110,0) : set(110,(get(5)-4)*200/16);\nget(5)<3.5?\nsetint(209,2) : setint(209,0);\n// Flow\nset(111,(get(6)-4)*2848000/16);\nget(6)<3.5?\nsetint(210,2) : setint(210,0);\n// Press\nset(112,((get(7)-4)*20/16)-10);\nget(7)<3.5?\nsetint(211,2) : setint(211,0);\n// Temp\nset(113,(get(8)-4)*150/16);\nget(8)<3.5?\nsetint(212,2) : setint(212,0);",
            "desc": "doc gia tri 4-20mA",
            "en": false
        },
